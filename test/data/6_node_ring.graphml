        <data key="image">bitcoindevproject/bitcoin:27.0</data>
        <data key="bitcoin_config">-uacomment=w2 -debug=validation</data>
    </node>
    <node id="3">
        <data key="image">bitcoindevproject/bitcoin:27.0</data>
        <data key="bitcoin_config">-uacomment=w3</data>
    </node>
    <node id="4">
        <data key="image">bitcoindevproject/bitcoin:27.0</data>
        <data key="bitcoin_config">-uacomment=w4</data>
    </node>
    <node id="5">
