  <graph edgedefault="directed">
    <node id="0">
        <data key="image">bitcoindevproject/bitcoin:27.0</data>
        <data key="bitcoin_config">-uacomment=w0 -debug=validation</data>
    </node>
    <node id="1">
        <data key="image">bitcoindevproject/bitcoin:27.0</data>
        <data key="bitcoin_config">-uacomment=w1 -debug=validation</data>
